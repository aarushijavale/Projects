      "source": [
        "#reverse a num\n",
        "num=int(input(\"enter a number:\"))\n",
        "rev_num=0\n",
        "while num>0:\n",
        "  digit=num%10\n",
        "  rev_num=rev_num*10+digit\n",
        "  num=num//10\n",
        "print(\"the reversed number is:\",rev_num)"
      ]
